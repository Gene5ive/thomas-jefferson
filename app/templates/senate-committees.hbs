<div class="col-md-8 col-md-offset-2">
  <br>
  <ul>
    {{#each committee in model}}
      {{#link-to 'committee' committee.committee_id}}
        <li>
          Committee Name:
          <span class="committee-name">{{committee.name}}</span>
        </li>
      {{/link-to}}
    {{/each}}
  </ul>
</div>
