<div class="col-md-8 col-md-offset-2">
  <ul>
    {{#each chamber in model}}
      {{#each committee in chamber}}
        <li>
          Committee Name:
          <span class="committee-name">{{committee.name}}</span>
          <br>
          Committee Chamber:
          <span class="committee-chamber">{{committee.chamber}}</span>
        </li>
      {{/each}}
    {{/each}}
  </ul>
</div>
